___SANDBOXED_JS_FOR_SERVER___

//Setup constants
const JSON = require('JSON');
const logToConsole = require('logToConsole');
const sendHttpRequest = require('sendHttpRequest');
const encodeUri = require('encodeUri');


//If debug_mode On, write complete JSON to Console
if (data.debugMode == true) {
  logToConsole(JSON.stringify({
    'Point': 'Zbozi.cz - debug - input data',
    'Body': data
  }));
}


//Setup array for POST request to zbozi.cz
var post_data = {
  'PRIVATE_KEY': data.PrivateKey,
  'sandbox': 'false',
  'orderId': data.OrderID,
  'email': data.email,
  'deliveryType': data.deliveryType,
  'deliveryPrice': data.deliveryPrice,
  'paymentType': data.paymentType,
  'otherCosts': data.otherCosts,
  'cart': []
};

//Convert products from ecommerce format to zbozi.cz format. If products exists.
if (typeof data.products != 'undefined') {
  data.products.forEach(function(produkt, index) {
    post_data.cart[index] = {
      'itemId': produkt.item_id,
      'productName': produkt.item_name,
      'unitPrice': produkt.price,
      'quantity': produkt.quantity
    } ;
  });
}

//When  I have sandbox
if (data.environment == 'sandbox') {
logToConsole(JSON.stringify({'Point': 'Zbozi.cz - debug - sandbox On'}));
  
  var url_api = 'https://sandbox.zbozi.cz/action/'+encodeUri(data.ShopId)+'/conversion/backend';
  post_data.sandbox = true ;
} else {
  var url_api = 'https://www.zbozi.cz/action/'+encodeUri(data.ShopId)+'/conversion/backend';
logToConsole(JSON.stringify({'Point': 'Zbozi.cz - debug - sandbox Off'}));  
}

//If debug_mode On, write complete JSON to Console
if (data.debugMode == true) {
  logToConsole(JSON.stringify({
    'Point': 'Zbozi.cz - debug - output data',
    'Body': post_data
  }));
}


//Send data to zbozi.cz
sendHttpRequest(url_api, (statusCode, headers, body) => {
  if (statusCode >= 200 && statusCode < 300) {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'Zbozi.cz - Success',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body,
        'PostData': post_data,
      }));
    }
    data.gtmOnSuccess();
  } else {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'Zbozi.cz - Failure',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body,
        'PostData': post_data,
      }));
    }
    data.gtmOnFailure();
  }},
  {
    headers: {'content-type': 'application/json'},
    method: 'POST', 
    timeout: 2000
   },
   JSON.stringify(post_data)
);


